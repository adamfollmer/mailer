<Window x:Class="NewMailer.NameNewGym"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewMailer"
        mc:Ignorable="d"
        Title="NameNewGym" Height="350" Width="525">
    <Grid>
        <Button x:Name="Continue" Content="Continue" HorizontalAlignment="Left" Margin="162,159,0,0" VerticalAlignment="Top" Width="75" Click="Continue_Click"/>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="274,159,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <Label x:Name="WhatsTheName" Content="What Is The Name Of The New Gym?" HorizontalAlignment="Left" Margin="162,30,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="get_GymName" HorizontalAlignment="Left" Height="28" Margin="120,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="277"/>
        <Label x:Name="MatchName" Content= "Please Remember This Must Match The Name From The CSV!!" HorizontalAlignment="Left" Margin="97,106,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
