<Window x:Class="NewMailer.Upload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewMailer"
        mc:Ignorable="d"
        Title="Upload" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="txtEditor" HorizontalAlignment="Left" Height="159" TextWrapping="Wrap" VerticalAlignment="Top" Width="170" Margin="162,91,0,0"/>
        <Button x:Name="Send" Content="Send Email" HorizontalAlignment="Left" Margin="217,269,0,0" VerticalAlignment="Top" Width="75" Click="SendEmail"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="33,188,0,0" Click="MassEmail"/>
    </Grid>
</Window>
