<Window x:Class="NewMailer.Upload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewMailer"
        mc:Ignorable="d"
        Title="Upload" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Select File/Send Email" HorizontalAlignment="Left" VerticalAlignment="Top" Width="155" Margin="178,85,0,0" Click="MassEmail" Height="76"/>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="207,210,0,0" VerticalAlignment="Top" Width="90" Click="Cancel_Click"/>
    </Grid>
</Window>
