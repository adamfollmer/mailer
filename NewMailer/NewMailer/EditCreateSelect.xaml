<Window x:Class="NewMailer.EditCreateSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewMailer"
        mc:Ignorable="d"
        Title="EditCreateSelect" Height="350" Width="525">
    <Grid>
        <Button x:Name="Create_Button" Content="Create New Gym" HorizontalAlignment="Left" Margin="299,103,0,0" VerticalAlignment="Top" Width="104" Click="Create_Button_Click"/>
        <ListBox x:Name="Gym_List" HorizontalAlignment="Left" Height="180" Margin="71,49,0,0" VerticalAlignment="Top" Width="157" MouseDoubleClick="Gym_List_MouseDoubleClick"/>
        <Button x:Name="Cancel_Button" Content="Cancel" HorizontalAlignment="Left" Margin="312,172,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Button_Click"/>
        <Label x:Name="Gym_Label" Content="Click A Gym To Edit" HorizontalAlignment="Left" Margin="95,18,0,0" VerticalAlignment="Top" Width="157"/>

    </Grid>
</Window>
